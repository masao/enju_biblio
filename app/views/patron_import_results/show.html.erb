<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.patron_import_result')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <b><%= t('activerecord.models.patron_import_file') %></b>
  <%= @patron_import_result.patron_import_file.patron_import_file_name %>
</p>

<p>
  <b><%= t('activerecord.models.patron') %></b>
  <%= link_to @patron_import_result.patron.full_name, @patron_import_result.patron if @patron_import_result.patron %>
</p>

<p>
  <b><%= t('activerecord.models.user') %></b>
  <%= link_to @patron_import_result.user.username, @patron_import_result.user if @patron_import_result.user %>
</p>

<p>
  <b><%= t('activerecord.attributes.patron_import_result.body') %></b>
  <%= @patron_import_result.body %>
  </p>

<p>
  <b><%= t('page.created_at') %></b>
  <%=l @patron_import_result.created_at %>
</p>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= back_to_index(flash[:page_info]) -%></li>
  </ul>
</div>
